#include <iostream>
using namespace std;

int main()
{
	int n, m, k;
	cin >> n >> m;
	_asm {
		mov ecx, n  //for(int i = 0;i<n;i++) ecx = i
		mov bx, 1 // bx-1,2,3,....
		mov eax, 1 //текущий ответ
		metka:mul bx // eax*=bx // 1*1
		inc bx //bx++
		loop metka
		//mov n,eax

		mov ecx,m
		mov bx,1
		sub ecx,n
		metka1:mul bx
		inc bx
		loop metka1
		//mov n,eax
		mov k,eax

		mov ecx,m
		mov bx,1
		mov eax,1
		metka2:mul bx
		inc bx
		loop metka2
		mov n,eax

	}
	cout << n/k;
}
