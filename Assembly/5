#include <iostream>
using namespace std;

float distance(float* t1, float* t2, int n) {
    float res = 0.0;
    _asm {
        mov ecx, n
        mov esi, t1
        mov edi, t2
        xorps xmm0, xmm0

        loop_start:
            movss xmm1, [esi]
            subss xmm1, [edi]
            mulss xmm1, xmm1
            addss xmm0, xmm1

            add esi, 4
            add edi, 4

            loop loop_start

            sqrtss xmm0, xmm0
            movss res, xmm0
    }
    return res;
}

int main() {
    setlocale(LC_ALL, "RUS");
    int n = 10;
    float t1[10], t2[10];
    cout << "Введите координаты первой точки: ";
    for (int i = 0; i < 10; i++) {
        cin >> t1[i];
    }
    cout << "Введите координаты второй точки: ";
    for (int i = 0; i < 10; i++) {
        cin >> t2[i];
    }
    float res = distance(t1, t2, 10);
    cout << "Расстояние между двумя точками: " << res << endl;
    return 0;
}
