#include <iostream>
using namespace std;

int main()
{
    setlocale(LC_ALL, "RUS");
    double a;
    unsigned n;
    cout << "Введите число = ";
    cin >> a;
    cout << "Введите степень = ";
    cin >> n;

    _asm
    {
        mov eax, n
        fld1
        fld a
        m1:
            test eax, 1
            jz m2
            fmul st(1), st(0)
        m2:
            shr eax, 1
            jz m3
            fmul st(0), st(0)
            jmp m1
        m3:
            fincstp
            fstp a
    }
    cout << a << endl;
}
